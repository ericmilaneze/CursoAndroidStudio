<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="institucional.cursoandroid.com.institucional">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AEmpresaActivity"
            android:label="@string/app_name_aempresa" />
        <activity
            android:name=".ServicosActivity"
            android:label="@string/app_name_servicos" />
        <activity android:name=".ClientesActivity" android:label="@string/app_name_clientes" />
        <activity android:name=".ContatoActivity" android:label="@string/app_name_contato"></activity>
    </application>

</manifest>